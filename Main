public class Main //Driver
{
    public static void main(String[] args)
    {
        Novel Secker = new Novel("George Orwell", "Harvill Secker", "1984", 328, 23, true);
        Magazine Vouge = new Magazine("Gaby Wood", "Vouge", "From Little Women to Marvel Superhero, Florence Pugh Is a New Kind of Breakout Star", 6, true);
        
        //Instantiate array
        Reading[] output = new Reading[2];
        output[0] = Secker;
        output[1] = Vouge;
        //Print out the array of different classes
        for (int i = 0; i < output.length; i++)
        {
            System.out.println(output[i]);
        }//end of for loop
    }//end of class
}//end of class
